{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@cpt/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@scss/*": [
        "src/scss/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@route/*": [
        "src/route/*"
      ],
      "@frame/*": [
        "src/frame/*"
      ],
    }
  },
  "include": [
    "src", "react-app-env.d.ts"
  ]
}
